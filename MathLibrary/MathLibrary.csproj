<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
	<PackageId>GoodCalc</PackageId>
	<Version>1.1.0</Version>
	<Authors>Kuchmatyg</Authors>
	<Company>KDD</Company>
	<Description>Allows you to perform mathematical operations</Description>
	<PackageTags>math arithmetic calculator</PackageTags>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<RepositoryUrl>https://github.com/Kuchmatyg/MathLib</RepositoryUrl>
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<GenerateDocumentationFile>true</GenerateDocumentationFile>
	<DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(PackageId).xml</DocumentationFile>
  </PropertyGroup>

</Project>
